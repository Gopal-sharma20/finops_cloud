# AWS Configuration
# Default AWS region for API calls
AWS_DEFAULT_REGION=us-east-1

# Comma-separated list of AWS CLI profile names to use
# These profiles should be configured in ~/.aws/credentials and ~/.aws/config
# Example: AWS_PROFILES=default,production,staging,dev
AWS_PROFILES=default

# Optional: Specific AWS credentials (if not using profiles)
# AWS_ACCESS_KEY_ID=your-access-key-id
# AWS_SECRET_ACCESS_KEY=your-secret-access-key

# GCP Configuration
# Your GCP project ID
GCP_PROJECT_ID=your-project-id

# Azure Configuration (if using Azure)
AZURE_MCP_SERVER_URL=http://localhost:8000

# AWS Configuration (if using AWS)
MCP_SERVER_URL=http://localhost:3001

# GCP MCP Server URL (if using GCP)
GCP_MCP_SERVER_URL=http://localhost:3002

# Next.js Configuration
NEXT_PUBLIC_API_URL=http://localhost:3000
